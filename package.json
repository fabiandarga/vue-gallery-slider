{
  "name": "vue-gallery-slider",
  "version": "1.0.2",
  "description": "A horizontal scrolling gallery.",
  "main": "dist/vue-gallery-slider.ssr.js",
  "browser": "dist/vue-gallery-slider.esm.js",
  "module": "dist/vue-gallery-slider.esm.js",
  "unpkg": "dist/vue-gallery-slider.min.js",
  "types": "vue-gallery-slider.d.ts",
  "files": [
    "dist/*",
    "vue-gallery-slider.d.ts",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "test": "ava",
    "test:watch": "ava --watch",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@types/debounce": "^1.2.0",
    "debounce": "^1.2.1",
    "ts-node": "^10.0.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "ava": "^3.15.0",
    "browser-env": "^3.3.0",
    "cross-env": "^7.0.3",
    "esm": "^3.2.25",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^3.8.3",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register",
      "./test/helpers/setup-browser-env.js"
    ],
    "files": [
      "test/**/*.spec.ts"
    ]
  }
}
